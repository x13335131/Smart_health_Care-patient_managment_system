	<h1>List of Doctors in country</h1>
	
	<div class="index-form">
		<div class="jumbotron">
		  <!--listing all doctors/users-->
		     <table class="data_table">
       <thead>
    <tr>
      <th>First name</th>
      <th>Surname</th>
      <th>Email </th>
      <th>Clinic</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
      <tr>
<% if @users.present? %>
   <% @users.each do |user| %>
  
        <td><%= user.fname %></td>
        <td><%= user.lname %></td>
        <td><%= user.email %></td>
        <td><%= user.clinic.name %></td>
      </tr>
       <% end %>
      </tbody>
    </table>
   
<% else %>
  <p>There are no users containing the term(s) <%= params[:search] %>.</p>
<% end %>
<br>
  <%= link_to 'Return to user profile', user_path(current_user)%>